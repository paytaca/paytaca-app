{
	"appId": "com.paytaca.app",
	"appName": "Paytaca",
	"bundledWebRuntime": false,
	"npmClient": "npm",
	"webDir": "www",
	"plugins": {
		"SplashScreen": {
			"launchShowDuration": 2000,
			"launchAutoHide": true,
			"androidScaleType": "CENTER_CROP",
			"showSpinner": false
		},
		"Keyboard": {
			"resize": "body",
			"style": "dark",
			"resizeOnFullScreen": true
		},
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		},
		"Camera": {
			"android": {
				"usePhotoPicker": true
			}
		}
	},
	"ios": {
		"scheme": "Paytaca"
	},
	"server": {
		"androidScheme": "https"
	},
	"packageClassList": [
		"BarcodeScanner",
		"NativeAudio",
		"AppPlugin",
		"CAPBrowserPlugin",
		"CAPCameraPlugin",
		"DevicePlugin",
		"GeolocationPlugin",
		"KeyboardPlugin",
		"PushNotificationsPlugin",
		"NativeBiometric"
	]
}
