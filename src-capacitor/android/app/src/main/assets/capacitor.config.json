{
	"appId": "com.paytaca.app",
	"appName": "Paytaca",
	"bundledWebRuntime": false,
	"npmClient": "npm",
	"webDir": "www",
	"plugins": {
		"SplashScreen": {
			"launchShowDuration": 0
		},
		"Keyboard": {
			"resize": "body",
			"style": "dark",
			"resizeOnFullScreen": true
		},
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		},
		"Camera": {
			"android": {
				"usePhotoPicker": true
			}
		}
	},
	"ios": {
		"scheme": "Paytaca"
	},
	"server": {
		"androidScheme": "https"
	}
}
