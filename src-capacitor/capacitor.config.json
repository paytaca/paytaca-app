{
  "appId": "com.paytaca.app",
  "appName": "Paytaca",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "plugins": {
    "Nfc": {
      "enabled": true
    },
    "SplashScreen": {
      "launchShowDuration": 0
    },
    "Keyboard": {
      "resize": "body",
      "style": "dark",
      "resizeOnFullScreen": true
    },
    "PushNotifications": {
      "presentationOptions": [
        "badge",
        "sound",
        "alert"
      ]
    }
  },
  "ios": {
    "scheme": "Paytaca"
  },
  "server": {
    "androidScheme": "https",
    "url": "http://192.168.1.240:9000"
  }
}